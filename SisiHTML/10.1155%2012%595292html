factors NF-\u003ci\u003e\u0026#x3ba;\u003c/i\u003eB and AP-1."},{"name":"dc.publisher","content":"Hindawi"},{"name":"dc.format","content":"text/html"},{"name":"dc.language","content":"en"},{"name":"dc.identifier","content":"https://doi.org/10.1155/2012/595292"},{"name":"dc.type","content":"Research Article"},{"name":"dc.date","content":"2012/11/08"},{"name":"dcterms.issued","content":"2012/11/08"},{"name":"prism.publicationDate","content":"2012/11/08"},{"name":"prism.volume","content":2012},{"name":"prism.section","content":"Research Article"},{"name":"prism.doi","content":"https://doi.org/10.1155/2012/595292"},{"name":"prism.issn","content":"1741-427X"},{"name":"dc.source","content":"Evidence-Based Complementary and Alternative Medicine"},{"name":"prism.publicationName","content":"Evidence-Based Complementary and Alternative Medicine"},{"name":"authors","content":"Chao-Bin Yeh | Ming-Ju Hsieh | ... | Shun-Fa Yang"}],"title":"Terminalia catappa Exerts Antimetastatic Effects on Hepatocellular Carcinoma through Transcriptional Inhibition of Matrix Metalloproteinase-9 by Modulating NF-κB and AP-1 Activity","current_url":"https://www.hindawi.com/journals/ecam/2012/595292/","site_url":"https://www.hindawi.com","gdprMsg":"","manuscript_link":"https://mts.hindawi.com/","footerData":{"status":200,"data":{"upper_menu_items":{"footer":[{"title":"About us","external":0,"relative":"/about","expanded":0,"enabled":1},{"title":"Contact us","external":0,"relative":"/about/contact","expanded":0,"enabled":1},{"title":"Partnerships","external":0,"relative":"/publishing-partnerships","expanded":0,"enabled":1},{"title":"Blog","external":0,"relative":"/blog","expanded":0,"enabled":1}],"footer-1":[{"title":"Journals","external":0,"relative":"/journals","expanded":0,"enabled":1},{"title":"Article Processing Charges","external":0,"relative":"/publish-research/authors/article-processing-charges","expanded":0,"enabled":1},{"title":"Print editions","external":0,"relative":"/print-editions","ex