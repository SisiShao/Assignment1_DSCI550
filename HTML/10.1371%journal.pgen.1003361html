    <li id="almCitations">
      <%= s.numberFormat(citationCount, 0) %>
      <div class="tools" data-js-tooltip-hover="trigger">
        <a class="metric-term" href="/plosgenetics/article/metrics?id=10.1371/journal.pgen.1003361#citedHeader">Citation</a>
